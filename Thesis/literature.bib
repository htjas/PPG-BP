  langid = {english},
  keywords = {blood pressure,electrocardiogram (ECG),photoplethysmography (PPG),pulse arrival time,pulse transit time},
  file = {C:\Users\htjas\Zotero\storage\N4SCSE6Z\Sharma et al. - 2017 - Cuff-Less and Continuous Blood Pressure Monitoring.pdf}
