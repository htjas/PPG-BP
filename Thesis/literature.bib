@article{aboyAutomaticBeatDetection2005,
  title = {An Automatic Beat Detection Algorithm for Pressure Signals},
  author = {Aboy, Mateo and McNames, James and Thong, Tran and Tsunami, Daniel and Ellenby, Miles S. and Goldstein, Brahm},
  date = {2005-10},
  journaltitle = {IEEE transactions on bio-medical engineering},
