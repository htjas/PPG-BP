@article{sviridovaHumanPhotoplethysmogramNew2015,
  title = {Human Photoplethysmogram: New Insight into Chaotic Characteristics},
  shorttitle = {Human Photoplethysmogram},
  author = {Sviridova, Nina and Sakai, Kenshi},
