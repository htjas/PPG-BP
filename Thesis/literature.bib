@inproceedings{reyesWirelessPhotoplethysmographicDevice2012,
  title = {Wireless Photoplethysmographic Device for Heart Rate Variability Signal Acquisition and Analysis},
  booktitle = {2012 {{Annual International Conference}} of the {{IEEE Engineering}} in {{Medicine}} and {{Biology Society}}},
  author = {Reyes, Ivan and Nazeran, Homer and Franco, Mario and Haltiwanger, Emily},
