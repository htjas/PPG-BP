@article{liangHypertensionAssessmentECG2018,
  title = {Hypertension {{Assessment}} via {{ECG}} and {{PPG Signals}}: {{An Evaluation Using MIMIC Database}}},
  shorttitle = {Hypertension {{Assessment}} via {{ECG}} and {{PPG Signals}}},
  author = {Liang, Yongbo and Chen, Zhencheng and Ward, Rabab and Elgendi, Mohamed},
